#!/bin/bash

# Source this file to:
# - Load openmpi library
# - Set compiler version
# - Load a script for common setup to all compilers

# Load openmpi library
. r.load.dot /fs/ssm/main/opt/openmpi/openmpi-3.1.2--hpcx-2.4.0-mofed-4.6--gcc

export COMPILER_name=gnu

COMPILER_version=$(project/compiler.sh gnu)
export COMPILER_version

. ./.common_setup gnu

